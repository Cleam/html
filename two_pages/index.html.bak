	<script type="text/javascript">
		var basePath = "${pageContext.request.contextPath }";

		function acceptupd() {
			$("#msg1").text("");
			$("#msg2").text("");
			$("#msg3").text("");
			var spwd = $("#spwd").val();
			var npwd1 = $("#npwd1").val();
			var npwd2 = $("#npwd2").val();
			if (spwd.trim().length <= 0) {
				$("#msg1").text(" *原密码不能为空");
				return;
			}
			if (npwd1.trim().length < 6 || npwd1.trim().length > 30) {
				$("#msg2").text(" *密码长度在6-30位");
				return;
			}
			if (npwd2.trim().length < 6 || npwd2.trim().length > 30) {
				$("#msg3").text(" *密码长度在6-30位");
				return;
			}
			if (npwd1 != npwd2) {
				$("#msg2").text(" *两次输入的密码不一致");
				$("#msg3").text(" *两次输入的密码不一致");
				return;
			}
			var flag = false;
			if (!flag) {
				flag = true;
				$
						.ajax({
							type : "POST",
							url : basePath + "/updatepwd.json",
							data : {
								'spwd' : spwd,
								'npwd' : npwd2
							},
							dataType : "json",
							contentType : "application/x-www-form-urlencoded; charset=utf-8",
							success : function(data) {
								if (data.flag == "error") {
									window.location.href = basePath
											+ "/index.jsp";
								}
								if (data.flag == false) {
									flag = false;
									alert(data.Msg)
								} else if (data.flag == true) {
									alert(data.Msg);
									window.location.href = basePath
											+ "/index.jsp";
								} else {
									flag = false;
									alert(data.Msg);
								}
							},
							error : function() {
								alert("error!");
								flag = false;
							}
						});
			}

		}

		function cancelupd() {
			$("#spwd").val("");
			$("#npwd1").val("");
			$("#npwd2").val("");
			$("#msg1").text("");
			$("#msg2").text("");
			$("#msg3").text("");
		}
		
		
